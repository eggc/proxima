<% @projects.each do |product| %>
  <div>
    <%= product.id %>
    <%= product.title %>
    <%= product.media %>
    <%= product.description %>
    <%= product.release_date %>
    <%= link_to("edit", edit_project_path(product)) %>
    <%= link_to("destroy", project_path(product), data: { turbo_method: :delete }) %>
  </div>
<% end %>

<%= link_to("new", new_project_path) %>
