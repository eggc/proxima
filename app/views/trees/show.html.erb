<ul class="menu rounded-box">
  <% @workspaces.each do |workspace| %>
    <li>
      <%= link_to(workspace.name, edit_workspace_path(workspace)) %>
      <ul>
        <li><%= link_to('Dots', dots_path) %></li>
        <ul>
          <% workspace.dots.each do |dot| %>
            <li><%= link_to(dot.content.truncate(40), edit_dot_path(dot)) %></li>
          <% end %>
        </ul>
        <!-- <li>characters</li>
             <ul>
             <li>1</li>
             <li>2</li>
             </ul> -->
      </ul>
    </li>
  <% end %>
</ul>
